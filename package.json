{
   "name": "baseline",
   "version": "0.1.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start:browser": "npx webpack",
      "start": "npx webpack-dev-server --config ./webpack.dev.js",
      "start:server": "nodemon --exec npx babel-node ./src/api/index.js",
      "build": "npx webpack --config ./webpack.prod.js",
      "lint": "npx eslint --ext .js --ext .jsx ./src",
      "test": "npx mocha --watch --require ./tests/mocha-setup.js ./src/**/test-*.js"
   },
   "engines": {
      "node": ">=10.0.0"
   },
   "app-ports": {
      "webpack-port": "8081",
      "node-port": "3000"
   },
   "babel": {
      "presets": [
         "@babel/env",
         "@babel/react"
      ],
      "plugins": [
         [
            "@babel/plugin-proposal-decorators",
            {
               "legacy": true
            }
         ],
         "@babel/plugin-proposal-object-rest-spread",
         "@babel/plugin-transform-destructuring",
         "@babel/plugin-proposal-class-properties",
         "@babel/plugin-syntax-dynamic-import",
         [
            "module-resolver",
            {
               "alias": {
                  "~": "./src/"
               }
            }
         ]
      ]
   },
   "author": "Philip Kuperberg",
   "license": "ISC",
   "dependencies": {
      "@babel/node": "^7.4.5",
      "@babel/polyfill": "^7.2.5",
      "autoprefixer": "^9.6.0",
      "chai": "^4.2.0",
      "classnames": "^2.2.5",
      "clean-webpack-plugin": "^3.0.0",
      "colors": "^1.3.3",
      "css-loader": "^2.1.1",
      "cssnano": "^4.1.10",
      "enzyme": "^3.10.0",
      "enzyme-adapter-react-16": "^1.14.0",
      "express": "^4.17.1",
      "font-awesome": "^4.7.0",
      "html-webpack-plugin": "^3.2.0",
      "jsdom": "^15.1.1",
      "lodash": "^4.17.11",
      "memoize-one": "^5.0.0",
      "mini-css-extract-plugin": "^0.7.0",
      "mocha": "^6.1.4",
      "nodemon": "^1.19.1",
      "postcss-loader": "^3.0.0",
      "prop-types": "^15.7.2",
      "react": "^16.8.4",
      "react-dom": "^16.8.4",
      "react-hot-loader": "^4.9.0",
      "react-router": "^5.0.1",
      "react-router-dom": "^5.0.1",
      "reselect": "^4.0.0",
      "resolve-url-loader": "^3.0.1",
      "sass": "^1.21.0",
      "sass-loader": "^7.1.0",
      "sinon": "^7.2.7",
      "style-loader": "^0.23.1",
      "webpack": "^4.33.0",
      "webpack-bundle-analyzer": "^3.3.2",
      "webpack-cli": "^3.3.3",
      "webpack-dev-server": "^3.7.1",
      "webpack-merge": "^4.2.1",
      "webpack-notifier": "^1.7.0",
      "webpack-shell-plugin": "^0.5.0",
      "file-loader": "^4.0.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.2.3",
      "@babel/core": "^7.4.5",
      "@babel/plugin-proposal-class-properties": "^7.3.4",
      "@babel/plugin-proposal-decorators": "^7.3.0",
      "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
      "@babel/plugin-syntax-dynamic-import": "^7.2.0",
      "@babel/plugin-transform-destructuring": "^7.3.2",
      "@babel/preset-env": "^7.4.5",
      "@babel/preset-react": "^7.0.0",
      "@babel/register": "^7.0.0",
      "@hot-loader/react-dom": "^16.8.4",
      "babel-eslint": "^10.0.1",
      "babel-loader": "^8.0.6",
      "babel-plugin-module-resolver": "^3.2.0",
      "eslint": "^5.15.1",
      "eslint-config-airbnb": "^17.1.0",
      "eslint-import-resolver-babel-module": "^5.0.1",
      "eslint-plugin-import": "^2.17.3",
      "eslint-plugin-jsx-a11y": "^6.2.1",
      "eslint-plugin-react": "^7.12.4"
   }
}
